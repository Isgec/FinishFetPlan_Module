<?xml version="1.0" encoding="utf-8"?>
<odoo>
<act_window
id="FinishFetPlanModule_JobRoutingTable_action"
name="Job Routing Data "
res_model="finishfetplanmodule.jobroutingtable" />
<menuitem id="FinishFetPlanModule_mainmenu" name="FINISH FET PLAN Module"/>

<menuitem
 id="finishfetplanmodule_JobRoutingtable_menu"
 name="Job Routing"
 action="FinishFetPlanModule_JobRoutingTable_action"
 parent="FinishFetPlanModule_mainmenu"
 sequence="1" />

<record id ="finishfetplanmodule_JobRoutingtable_view_form" model="ir.ui.view">
   <field name="name">Application Table Name Form</field>
   <field name="model">finishfetplanmodule.jobroutingtable</field>
   <field name="arch" type="xml">
         <form>
               <sheet>
                   <group col="4">
                       <field name="name"/>
                       <field name="sequence_no"/>
                       <field name="colour"/>
                   </group>
               </sheet>
	    </form>
   </field>
</record>

<record id="finsihfetplanmodule_JobRoutingtable_view_tree" model="ir.ui.view">
  <field name="name">Application Table Name List</field>
  <field name="model">finishfetplanmodule.jobroutingtable</field>
  <field name="arch" type="xml">
    <tree>
      <field name="name"/>
      <field name="sequence_no"/>
        <field name="colour"/>
    </tree>
    </field>
</record>


</odoo>
