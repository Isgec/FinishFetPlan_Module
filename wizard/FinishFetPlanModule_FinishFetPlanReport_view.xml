<?xml version="1.0" ?>

<odoo>
<act_window
id="finishfetplanmodule_finishfetplanreport_export"
name="Generate Finish Fet Plan Report"
res_model="finishfetplanmodule.finishfetplanreport" />
	<menuitem
            id="finishfetplanmodule_finishfetplanreport_menu"
            name="Genrate Complete Excel"
            action="finishfetplanmodule_finishfetplanreport_export"
            parent="FinishFetPlanModule_mainmenu"
            sequence="5" />

    	<!--Export Form & Action -->

        <record id="view_finishfetplanmodule_finishfetplanreport" model="ir.ui.view">
            <field name="name">Dashboard-1</field>
            <field name="model">finishfetplanmodule.finishfetplanreport</field>
            <field name="arch" type="xml">
                <form string="Dashboard">
					<sheet>
					<group col="3">
							<field name="from_dt"/>
							<button name="button_excel" string=" Genrate Report" type="object" class="oe_highlight"  />

					</group>
				<br /><br /><br />	<group col="3">
							<field name="upload_file"/>
							<button name="upload_excel" string=" Upload Report" type="object" class="oe_highlight"  />

					</group>
					<group col="2">
						<button string="Cancel" class="oe_link" special="cancel" />
					</group>

					</sheet>
                 </form>
            </field>
        </record>

        <record id="excel_finishfetplanmodule_finishfetplanreport" model="ir.ui.view">
			<field name="name">view.ffpreport</field>
			<field name="model">view.ffpreport</field>
			<field name="arch" type="xml">
				<form string="popup">
					<sheet>
						<group col="4" colspan="2">
							<field name="name"/>
							<field name="datas_fname" filename="file_name"/>
							<field name="file_name" invisible="1"/>
						</group>
					</sheet>
				</form>
			</field>
		</record>

</odoo>

